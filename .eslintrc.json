{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["plugin:react/recommended", "airbnb"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": ["react", "@typescript-eslint"],
	"rules": {
		"quotes": "off",
		"@typescript-eslint/quotes": "error"
	}
}
